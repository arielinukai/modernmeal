body{
	margin:0;
	background:#e3ebd3;
	font:11px Helvetica, Arial, sans-serif;
	color:#a6a6a6;
	min-width:900px;
}
a{
	color:#a6a6a6;
	text-decoration:none;
}
a:hover{text-decoration:none;}
img{border-style:none;}
input,
textarea,
select{
	font:12px helvetica, arial, sans-serif;
	vertical-align:middle;
	color:#a6a6a6;
}
form,fieldset{
	margin:0;
	padding:0;
	border-style:none;
}
.wrapper{
	width:100%;
	overflow:hidden;
	background:#91b063 url(/images/bg-wrapper.jpg) no-repeat 50% 0;
}
.container{
	width:886px;
	margin:0 auto;
	padding:0 14px 0 0;
}
#header{
	width:872px;
	overflow:hidden;
	position:relative;
	padding:22px 0 12px 9px;
}
.logo{
	text-indent:-9999px;
	overflow:hidden;
	float:left;
	width:411px;
	height:118px;
	margin:0;
}
.logo a{
	display:block;
	height:100%;
	background:url(/images/logo.png) no-repeat;
	cursor:pointer;
}
.nav-holder{
	background:url(/images/bg-top-nav.gif) no-repeat;
	float:right;
	position:absolute;
	top:0;
	right:0;
}
#nav{
	background:url(/images/bg-top-nav.gif) no-repeat 100% -36px;
	margin:0;
	float:left;
	padding:0 17px 0 19px;
	height:36px;
	list-style:none;
}
#nav li{
	padding:10px 10px 0 8px;
	float:left;
}
#nav li a{
	font-size:11px;
	color:#fff;
	font-weight:bold;
	text-transform:uppercase;
}
.logo-area{
	width:100%;
	overflow:hidden;
}
.logo-area .slogan{
	float:right;
	font-size:14px;
	color:#fff;
	padding:50px 0 0;
	word-spacing:-2px;
}
.logo-area .slogan cufon{float:left;}
.main-container{
	width:872px;
	padding:0 0 20px 15px;
}
.main-container .main-t{
	background:url(/images/bg-main-container-t.png) no-repeat;
	height:5px;
	overflow:hidden;
}
.main-container .main-b{
	background:url(/images/bg-main-container-b.png) no-repeat;
	height:26px;
	overflow:hidden;
}
#main{
	width:861px;
	padding:0 5px 0 6px;
	position:relative;
	background:#fff url(/images/bg-main.gif) repeat-x;
}
.fade-gallery{
	width:861px;
	position:relative;
	height:351px;
	margin:0 0 26px;
}
.fade-gallery .gallery{
	margin:0;
	padding:0;
	list-style:none;
	height:351px;
	overflow:hidden;
	position:relative;
}
.fade-gallery .gallery li{
	position:absolute;
	top:0;
	width:100%;
	height:351px;
	left:0;
	display:none;
}
.fade-gallery .gallery li.active {
	display:block;
}
.fade-gallery .gallery li img{display:block;}
.fade-gallery .steps-list{
	margin:0;
	padding:0;
	list-style:none;
	position:absolute;
	left:0;
	top:51px;
}
.fade-gallery .steps-list li{
	height:61px;
	vertical-align:top;
	overflow:hidden;
	margin:0 0 1px;
}
.fade-gallery .steps-list li a{
	background:url(/images/bg-steps-list.png) no-repeat;
	width:144px;
	height:61px;
	display:block;
	padding:14px 0 0 10px;
	cursor:pointer;
}
.steps-list li a .number{
	width:29px;
	height:33px;
	float:left;
	text-align:center;
	line-height:32px;
	color:#ccba83;
	font-size:22px;
	padding:0 0 0 4px;
	background:url(/images/bg-number.png) no-repeat;
	margin:0 9px 0 0;
}
.steps-list li.active .number,
.steps-list li a:hover .number{
	color:#b1b1b1;
	background:url(/images/bg-number-hover.png) no-repeat;
}
* html .steps-list li a .number{margin-right:6px;}
.steps-list li .text1{
	display:block;
	color:#fbe9b2;
	font-size:12px;
	line-height:12px;
	padding:2px 0 0;
}
.steps-list li .text2{
	display:block;
	color:#fbe9b2;
	font-size:20px;
	text-transform:uppercase;
	font-weight:normal;
	line-height:19px;
}
.steps-list .last-button .text2{
	font-size:16px;
	line-height:20px;
}
.steps-list li.active a,
.steps-list li a:hover{
	background:url(/images/bg-steps-list-hover.png) no-repeat;
	width:144px;
	text-decoration:none;
	padding-left:34px;
}
.steps-list li.active .text1,
.steps-list li.active .text2,
.steps-list li a:hover .text1,
.steps-list li a:hover .text2{color:#fff;}
.fade-gallery .login-box-holder{
	width:255px;
	height:302px;
	position:absolute;
	right:7px;
	top:-21px;
}
.fade-gallery .login-box{
	width:255px;
	height:302px;
	background:url(/images/bg-login-box.png) no-repeat;
}
#login{
	overflow:hidden;
	padding:18px 14px 0 16px;
	width:217px;
	position:relative;
	font-size:12px;
}
#login .heading-holder{
	border-bottom:1px solid #ededed;
	padding:0 0 19px 7px;
	overflow:hidden;
	width:210px;
	margin:0 0 14px;
}
#login .heading-holder h3{
	font-size:28px;
	color:#afcd78;
	font-weight:normal;
	margin:0;
}
#login .heading-holder h3 cufon{float:left;margin:0 0 -8px;}
#login .heading-holder h3 span{font-size:18px;}
#login .row{
	padding:0 0 6px 38px;
	width:179px;
	overflow:hidden;
}
#login .input-holder{
	float:left;
	background:url(/images/bg-input-holder.png) no-repeat;
	width:137px;
	height:27px;
}
#login .text{
	float:left;
	width:117px;
	height:15px;
	padding:6px 10px;
	background:none;
	border:0;
	position:relative;
	color:#c5c5c5;
}
#login .submit{
	font-weight:bold;
	color:#fff;
	background:url(/images/bg-submit.png) no-repeat;
	width:76px;
	height:27px;
	text-align:center;
	padding:0;
	border:0;
	text-transform:uppercase;
	cursor:pointer;
	float:left;
	margin:0 0 0 29px;
	display:inline;
}
#login .check-holder{
	height:1%;
	overflow:hidden;
	padding:4px 0 8px 54px;
}
#login .check-holder label{float:left;}
#login .link-holder{
	width:100%;
	overflow:hidden;
	text-align:center;
	padding:0 0 9px;
}
#login .link-holder a{color:#b2b2b2;}
#login .register-box{
	height:1%;
	overflow:hidden;
	border-top:1px solid #e7e7e7;
	padding:11px 0 0 8px;
}
.register-box .info-text{
	float:left;
	padding:5px 9px 0 0;
}
.register-box .register{
	float:left;
	background:url(/images/bg-register.png) no-repeat;
	width:85px;
	height:27px;
	text-align:center;
	line-height:26px;
	color:#fff;
	font-weight:bold;
	cursor:pointer;
}
#main .box-holder{
	padding:0 25px 0 23px;
	width:813px;
	overflow:hidden;
}
.box-holder .columns-box{
	width:505px;
	float:left;
}
.columns-box .info-box{
	height:1%;
	overflow:hidden;
	padding:7px 14px 24px 0;
}
.columns-box .info-box h3{
	float:left;
	color:#70716d;
	font-size:28px;
	margin:2px 0 0;
	word-spacing:-2px;
}
.columns-box .info-box h3 cufon{float:left;}
.columns-box .info-box .test-run{
	color:#fff;
	font-size:12px;
	font-weight:bold;
	text-align:center;
	line-height:28px;
	background:url(/images/bg-test-run.gif) no-repeat;
	width:143px;
	height:29px;
	float:right;
}
.box-holder .ad-box{
	width:300px;
	float:right;
}
.ad-box .ad-title{
	color:#a2c465;
	display:block;
	font-size:10px;
	margin:0 0 4px;
	text-transform:uppercase;
}
.ad-box img{display:block;}
#main .threecolumns{
	width:490px;
	overflow:hidden;
	margin:0 0 22px;
	background:url(/images/bg-threecolumns.gif) repeat-y -483px 0;
}
.threecolumns .holder{background:url(/images/bg-threecolumns.gif) no-repeat;}
.threecolumns .frame{
	background:url(/images/bg-threecolumns.gif) no-repeat -966px 100%;
	width:103%;
	overflow:hidden;
	padding:0 0 18px;
	min-height:97px;
}
* html .threecolumns .frame{
	height:97px;
	overflow:visible;
}
.threecolumns .text-column{
	width:154px;
	float:left;
}
.threecolumns .text-column.center{
	width:130px;
	padding:0 30px 0 26px;
}
.threecolumns h4{
	margin:0 0 4px;
	font-size:15px;
	overflow:hidden;
	height:1%;
}
.threecolumns h4 a{
	float:left;
	padding:8px 0 0 23px;
	cursor:pointer;
	position:relative;
}
.threecolumns h4 a:hover{text-decoration:none;}
.threecolumns h4 a cufon{float:left;}
.threecolumns h4.profile a{
	color:#ea691c;
	background:url(/images/bg-icons.gif) no-repeat;
}
.threecolumns h4.add a{
	color:#c9bb23;
	background:url(/images/bg-icons.gif) no-repeat -182px -21px;
}
.threecolumns h4.search a{
	color:#12aecf;
	background:url(/images/bg-icons.gif) no-repeat -340px 0;
	padding:8px 0 0 17px;
}
.threecolumns h4.planner a{
	color:#c72323;
	background:url(/images/bg-icons.gif) no-repeat 0 -138px;
	padding-left:19px;
}
.threecolumns h4.grocery a{
	color:#9566c0;
	background:url(/images/bg-icons.gif) no-repeat -182px -181px;
}
.threecolumns h4.mobile a{
	color:#2b8a5c;
	background:url(/images/bg-icons.gif) no-repeat -344px -138px;
}
.threecolumns p{
	margin:0;
	font-size:12px;
	line-height:14px;
}
.social-box{
	width:860px;
	background:url(/images/bg-social-box.gif) repeat-y -860px 0;
}
.social-box .holder{background:url(/images/bg-social-box.gif) no-repeat;}
.social-box .frame{
	background:url(/images/bg-social-box.gif) no-repeat -1720px 100%;
	overflow:hidden;
	min-height:110px;
	text-align:center;
	padding:13px 0 10px;
}
* html .social-box .frame{
	height:110px;
	overflow:visible;
}
.social-box .social-list{
	width:100%;
	overflow:hidden;
	text-align:left;
}
.social-list .social-section{
	padding:0 0 0 24px;
	width:185px;
	float:left;
}
.social-list .social-section img{
	float:left;
	margin:0 7px 0 0;
}
.social-list .text-wrapp{
	overflow:hidden;
	padding:2px 0 0;
}
.social-list .text-wrapp p{
	margin:0;
	color:#858585;
	font-size:12px;
	line-height:14px;
}
.social-box h4{
	margin:0 0 20px;
	font-size:18px;
	color:#858585;
	font-weight:normal;
	word-spacing:-2px;
}
.social-box h4 cufon{margin:0 !important;}
#twocolumns{
	padding:0 0 25px 2px;
	width:884px;
	overflow:hidden;
}
.content-holder{
	width:694px;
	float:right;
}
.content-t{
	background:url(/images/bg-content-t.png) no-repeat;
	overflow:hidden;
	height:6px;
}
.content-b{
	background:url(/images/bg-content-b.png) no-repeat;
	height:27px;
	overflow:hidden;
}
#content{
	width:694px;
	position:relative;
	overflow:hidden;
}
#content .gradient{
	top:0;
	left:0;
	position:absolute;
	z-index:1;
	width:100%;
	height:100%;
}
* html .gradient{height:auto;}
* +html .gradient{height:auto;}
#content .block-container{
	width:100%;
	overflow:hidden;
	padding:9px 0 0;
	position:relative;
	z-index:2;
}
.block-container .trigger-block{
	border-bottom:1px solid #eaeaea;
	padding:10px 0 0 18px;
}
.block-container .trigger-block.entry{
	border:0;
	margin:0 0 -6px;
}
.block-container .trigger{
	margin:0;
	font-size:20px;
	color:#c8ba23;
	overflow:hidden;
	height:1%;
	background:url(/images/bg-trigger.gif) no-repeat -788px 12px;
	word-spacing:-2px;
	padding:0 0 8px 22px;
}
.block-container .trigger.active{
	background:url(/images/bg-trigger.gif) no-repeat 0 16px;
	padding-left:22px;
}
.block-container .trigger cufon{float:left;}
.block-container .trigger a{
	color:#c8ba23;
	background:url(/images/icons-text-container.gif) no-repeat;
	float:left;
	padding:8px 0 5px 36px;
	cursor:pointer;
	position:relative;
}
.block-container .trigger-block.entry .trigger a{
	background:url(/images/icons-text-container.gif) no-repeat -780px 3px;
	padding-left:21px;
}
.block-container .trigger a:hover{text-decoration:none;}
.block-container .toggle-container{
	font-size:12px;
	position:relative;
	width:100%;
	overflow:hidden;
}
.toggle-container .block{padding:7px 38px 15px 23px;}
.toggle-container p{
	margin:0 0 15px;
	line-height:14px;
}
.toggle-container .text-container{
	width:100%;
	padding:0 0 11px;
	overflow:hidden;
	position:relative;
}
.toggle-container .text-container .area{
	border:1px solid #dbdbdb;
	width:582px !important;
	background:#fff;
	font-size:11px;
	padding:5px 10px 0 21px;
	line-height:13px;
	margin:0;
	position:relative;
	overflow:auto;
	height:259px !important;
	text-align:left;
	float:left;
}
.toggle-container .link-holder{
	width:100%;
	overflow:hidden;
}
.link-holder .insert-link{
	float:right;
	color:#c8ba23;
	font-size:14px;
	background:url(/images/bg-insert-link.gif) no-repeat 0 50%;
	text-transform:uppercase;
	padding:0 0 0 27px;
	line-height:16px;
	word-spacing:-2px;
}
#content .block-wrapper{
	height:1%;
	overflow:hidden;
	padding:0 7px 12px 5px;
	position:relative;
	z-index:2;
}
#content .contact-info{
	height:1%;
	padding:21px 13px 6px 14px;
	overflow:hidden;
}
.contact-info .list-box{
	width:449px;
	float:left;
}
.list-box .text-wrapp{
	width:100%;
	overflow:hidden;
	padding:0 0 27px;
}
.list-box .title1{
	float:left;
	background:url(/images/bg-icons.gif) no-repeat -182px -181px;
	color:#9566c0;
	overflow:hidden;
	height:1%;
	font-size:20px;
	margin:0 19px 0 0;
	padding:0 0 0 25px;
}
.list-box .title1 cufon{float:left;}
.list-box .add-box{
	width:100%;
	overflow:hidden;
}
.add-box .add-text{
	color:#9a9a9a;
	float:left;
	background:url(/images/bg-add-item.gif) no-repeat;
	text-transform:uppercase;
	padding:3px 0 4px 27px;
	font-size:12px;
}
.add-box .item-list{
	margin:0 0 0 -2px;
	display:inline;
	padding:0;
	line-height:15px;
	list-style:none;
	float:left;
	overflow:hidden;
}
.add-box .item-list li{
	float:left;
	font-size:12px;
	font-weight:bold;
	background:url(/images/separator-item-list.gif) no-repeat 0 50%;
	margin:0 1px 0 -1px;
	padding:3px 9px 2px 11px;
}
.add-box .item-list li a{
	color:#9566c0;
	text-transform:uppercase;
}
.list-box .add{
	background:url(/images/bg-add-list.gif) no-repeat 0 2px;
	text-transform:uppercase;
 	color:#9a9a9a;
	font-size:12px;
	float:left;
	padding:1px 0 0 16px;
}
.contact-info .address-block{
	float:right;
	padding:1px 0 0;
	width:176px;
}
.contact-info .address-block h3{
	margin:0 0 4px;
	font-size:14px;
	color:#9566c0;
	text-transform:uppercase;
	overflow:hidden;
	height:1%;
	word-spacing:-2px;
}
.contact-info .address-block h3 cufon{float:left;}
.contact-info .address-block h4{
	margin:0 0 4px;
	font-size:12px;
	color:#9566c0;
	overflow:hidden;
	height:1%;
}
.contact-info .address-block h4 cufon{float:left;}
.address-block address{
	font:12px/14px Helvetica, Arial, sans-serif;
	overflow:hidden;
	width:100%;
}
.address-block address span{
	display:block;
	overflow:hidden;
	height:1%;
	margin:0 0 2px;
}
.address-block address span cufon{float:left;}
.address-block address span a{
	color:#9566c0;
	font-size:10px;
	position:relative;
	cursor:pointer;
}
.table-holder{
	width:682px;
	background:#fff;
}
.table-holder .table-block{
	background:url(/images/bg-table.gif) no-repeat 50% 100%;
	height:1%;
	overflow:hidden;
	padding:16px 31px 21px 9px;
	margin:0 0 20px;
}
.table-block .grocery-table{
	width:100%;
	border-collapse:collapse;
}
.grocery-table td,
.grocery-table th{
	padding:0;
	text-transform:uppercase;
}
.grocery-table th{
	font-size:14px;
	font-weight:normal;
	vertical-align:top;
	padding:0 0 13px;
}
.grocery-table td{
	font-size:12px;
	color:#9566c0;
	text-align:center;
	border-bottom:1px solid #d2d2d2;
	padding:9px 0 10px;
}
.grocery-table.small td{padding:5px 0 8px;}
.grocery-table .first{
	width:10px;
	border:0;
	vertical-align:middle;
}
.grocery-table .second{
	text-align:left;
	text-transform:uppercase;
	font-size:14px;
	width:238px;
	padding-left:4px;
}
.grocery-table.small td.second{padding-left:4px;}
.grocery-table .second span a{
	color:#9566c0;
	cursor:pointer;
}
.grocery-table .second span a:hover{text-decoration:underline;}
.grocery-table th.first{padding-top:3px;}
.grocery-table th.second{padding:0 0 0 23px;}
.grocery-table .second img{
	margin:0 2px;
	display:inline-block;
	vertical-align:middle;
}
.grocery-table .second span{
	display:inline-block;
	vertical-align:middle;
	width:210px;
}
.grocery-table .third{width:135px;}
.grocery-table .last,
.grocery-table.small .last{
	width:55px;
	padding:0 0 0 52px;
}
.grocery-table .checkboxArea,
.grocery-table .checkboxAreaChecked{margin:0 !important;}
.grocery-table .last img{
	vertical-align:middle;
	margin:0 4px 0 0;
}
.table-holder .link-holder{
	height:1%;
	overflow:hidden;
	padding:0 0 0 7px;
}
.table-holder .add-list{
	background:url(/images/bg-add-list.png) no-repeat;
	width:194px;
	height:27px;
	padding:0 0 0 40px;
	color:#fff;
	font-size:12px;
	line-height:26px;
	display:block;
	cursor:pointer;
}
.table-holder .table-container{
	background:url(/images/bg-table.gif) no-repeat 50% 100%;
	height:1%;
	padding:14px 14px 20px 16px;
	overflow:hidden;
}
.table-container .title-holder{
	width:100%;
	overflow:hidden;
	padding:0 0 15px;
}
.title-holder .recipe-title{
	float:left;
	font-size:20px;
	color:#9a9a9a;
	text-transform:uppercase;
	margin:6px 0 0 6px;
	word-spacing:-3px;
	display:inline;
}
.title-holder .recipe-title cufon{float:left;}
.table-container .links-box{float:right;}
.links-box .print{
	float:left;
	background:url(/images/bg-print.gif) no-repeat;
	font-size:12px;
	color:#9a9a9a;
	text-transform:uppercase;
	padding:2px 0 4px 27px;
	margin:4px 0 0;
}
.links-box .send{
	background:url(/images/bg-send-link.png) no-repeat;
	width:206px;
	height:27px;
	padding:0 10px 0 0;
	text-align:right;
	color:#fff;
	font-size:12px;
	line-height:26px;
	float:left;
	margin:0 0 0 10px;
	display:inline;
	cursor:pointer;
}
.table-container .recipe-table{
	width:100%;
	border-collapse:collapse;
	font-size:10px;
	color:#9a9a9a;
}
.recipe-table th,
.recipe-table td{padding:0;}
.recipe-table th{
	padding:0 0 11px;
	vertical-align:top;
	font-size:10px;
	text-transform:uppercase;
	color:#9a9a9a;
}
.recipe-table td{
	border-bottom:2px solid #ebebeb;
	padding:10px 0 11px;
	text-align:center;
}
.recipe-table .no-border td{border:0;}
.recipe-table .mark td{
	background:url(/images/bg-table-title.gif) repeat-x;
	font-size:12px;
	text-transform:uppercase;
	color:#fff;
	border:0;
	padding:5px 0 9px 9px;
	text-align:left;
}
.recipe-table td.table-heading{
	font-size:14px;
	padding-bottom:8px;
}
.recipe-table td.first{font-size:12px;}
.recipe-table td.second{
	color:#c8ba23;
	width:160px;
}
.recipe-table td.second a{color:#c8ba23;}
.recipe-table .first{
	padding:0 0 0 9px;
	text-align:left;
